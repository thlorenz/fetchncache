{
  "name": "fetchncache",
  "version": "0.1.2",
  "description": "Fetches a resource from a server and stores it inside a redis cache and the next time it grabs it from there",
  "main": "index.js",
  "scripts": {
    "test": "echo 'The tests require to have a redis running on REDIS_HOST:6379' && tap test/*.js",
    "commander": "redis-commander --redis-port 6379 --redis-host 33.33.33.100 --port 3000",
    "tunnel": "lt --port 3000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/fetchncache.git"
  },
  "homepage": "https://github.com/thlorenz/fetchncache",
  "dependencies": {
    "redis": "~0.10.1",
    "hiredis": "~0.1.16",
    "xtend": "~2.1.2",
    "hyperquest": "~0.3.0"
  },
  "devDependencies": {
    "nave": "~0.4.3",
    "tape": "~1.0.4",
    "tap": "~0.4.3",
    "redis-commander": "~0.2.1",
    "nock": "~0.27.2"
  },
  "keywords": [
    "redis",
    "cache",
    "service",
    "resource",
    "store",
    "expire",
    "proxy"
  ],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/fetchncache/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.10"
  }
}
